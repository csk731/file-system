# Render Environment Variables
# These will be automatically set by render.yaml

# Database (Render will provide this automatically)
# DATABASE_URL=postgresql://...

# Security (Render will generate this)
# SECRET_KEY=...

# File Upload Settings
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=104857600

# CORS Settings (Render will set this)
CORS_ORIGINS=https://fileupdown-frontend.onrender.com

# Frontend API URL (Render will set this)
VITE_API_URL=https://fileupdown-backend.onrender.com 